<% @the_web_page_title = "Edit category: #{@event.name}" %>


 
<%= render 'shared/event_error_messages_html.erb' %>

<div class="subject edit">
  <h2>Edit Event</h2>

 

  <%= form_for(:event, :url => {:action => 'updateEvents', :id => @event.id}) do |f| %>
  
  <%= render(:partial => "form", :locals => {:f => f}) %>
    
 
      <%= submit_tag "Update Event",:class=>'btn' %>
 
    <%= link_to("Cancel", {:action => 'index'}, :class => 'btn') %>
  <% end %>
</div>