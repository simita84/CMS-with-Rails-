<html>
 

<body>
  

<h2>Manage Admins</h2>
 
 <br>	<br>
	<%=link_to("Create Admin",{:action=>'newAdmin'},:class=>"btn")%>  
 <%=link_to("Cancel",{:controller=>'access',:action=>'menu'},:class=>"btn")%> 

	
	<br>	<br>
<table class="table table-striped">

	<th>		<%="First Name"%> </th>
	<th>	<%="Last Name"%>  </th>   
	<th>	<%="User Name"%>  </th>  
		<th>	<%="EmailId"%>  </th>
		<th>	<%="Created On"%>  </th>
	 	 
	 	 
			<th>		<%="Actions"%></th>
 	 
 
<tr>
			<%= will_paginate @admins %>
	<%@admins.each do |admin|%>
	<td><%=admin.first_name%>        </td>  
	   	<td><%=admin.last_name%>        </td>      				 
        	<td><%=admin.username%>        </td>    		 		  
	<td><%=admin.emailId%>        </td>  	 
	  		<td><%=admin.created_at.to_time.strftime('%e %b %Y %H:%m')%>        </td> 
	 <td > <%=link_to("EDIT",{:action=>'editAdmin',:id=>admin.id},:class=>"btn")%> 
		 <%=link_to("DELETE",{:action=>'deleteAdmin',:id=>admin.id},:class=>"btn")%>	</td>	  
			 	   
		</tr>
		
	<%end%>
     
	
	</table>
	
			<%= will_paginate @admins %>
	  
	 

 
</html>
 
